{
  "name": "todo.kgenots.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "build:prod": "set NEXT_PUBLIC_STAGE=prod && rm -rf node_modules/.cache && next build && touch build/.nojekyll && echo todo.kgenots.com > build/CNAME",
    "deploy": "set NEXT_PUBLIC_STAGE=prod && rm -rf node_modules/.cache && next build && touch build/.nojekyll && echo todo.kgenots.com > build/CNAME && gh-pages -d build -b gh-pages -r git@github.com:kgenots/todo.kgenots.com.git"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "@mui/x-date-pickers": "^6.10.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "next-redux-wrapper": "^8.1.0",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.2",
    "react-hook-form-mui": "^6.4.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@types/redux-logger": "^3.0.9",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "gh-pages": "^5.0.0",
    "prettier": "^3.0.0",
    "redux-logger": "^3.0.6"
  }
}
